////
// @FILE:         f_init.mnu
// @DESCRIPTION:  Application initiation
// 

FUNC(f_Init) {

  INC("EMMC/_inc.mnu");

  RUN(f_InitInt);
  
  RUN(f_InitStart);
}

FUNC(f_InitInt) {

  INT(i_Main,TIMER1,f_Main);



}

FUNC(f_InitStart) {

  RUN(_cReloadCol);
  RUN(_cReloadTheme);

  SHOW(p_Splash);
  
  WAIT(1000);
  LOAD(TIMER1,nTimRefr);
  SHOW(p_Main);
}